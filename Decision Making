                 Budget
                 
 #python
A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
A=A*350.34
B=B*230.90
C=C*190.55
D=D*125.30
E=E*180.90
T=A+B+C+D+E
if(T<=15000):
    print("Yes")
else:
    print("No")  
    
    
    
    
                       Sece Dine
    
#python
d=input()
r=int(input())
if d=='front':
    if r==1:
        print("Left Handed")
    else:
        print("Right Handed")
elif d=='rear':
    if r==1:
        print("Right Handed")
    else:
        print("Left Handed")    
        
        
        
        
        
        
                          Intersection Of a Circle
      
#python
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
r=r1+r2
y=abs(y2-y1)
x=abs(x2-x1)
if x==r or y==r:
    print("The circles are tangential")
elif r>x or r>y:
    print("The circles overlap")
else:
    print("The circles do not overlap") 
    
    
    
    
       Microwave Oven
    
#python
n=int(input("Enter the number of items\n"))
h=float(input("Enter the single item heating time\n"))
if n==2:
    h=h*1.5
    print(f"The recommended heating time is {'%.2f'%h} ")
elif n==3:
    h=h*2
    print(f"The recommended heating time is {'%.2f'%h}")
else:
    print("Number of items is more")
    
    
    
    
    
    
        
    
       
        
    
